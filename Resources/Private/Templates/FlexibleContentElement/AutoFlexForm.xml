{namespace fed=Tx_Fed_ViewHelpers}
<T3DataStructure>
	<meta>
	    <langDisable>1</langDisable>
	</meta>
	<ROOT>
	    <type>array</type>
	    <el>
	<f:for each="{fce}" as="fcegroup" iteration="iteration">
	      <f:for each="{fcegroup.attributes}" as="fceattribute" iteration="fceiteration">
			<{fceattribute.name}>
			<TCEforms>
				<label>{fceattribute.label}</label>
				<config>
					<type>input</type>
					<default>{fceattribute.default}</default>
				</config>
			</TCEforms>
			</{fceattribute.name}>
		  </f:for>
	</f:for>
	    </el>
	</ROOT>
</T3DataStructure>